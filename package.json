{
  "name": "pentonville",
  "version": "1.0.16",
  "description": "Pentonville",
  "keywords": [
    "Pentonville",
    "React",
    "tab",
    "tabindex",
    "input",
    "HTML",
    "element",
    "focus"
  ],
  "main": "./index.js",
  "author": "Jonathan Perry for Sequence Media Limited <sequencemedia@sequencemedia.net>",
  "license": "MIT",
  "engines": {
    "node": ">=12.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/sequencemedia/pentonville.git"
  },
  "scripts": {
    "build": "babel src -d lib",
    "build:watch": "babel src -d lib -w",
    "prepublishOnly": "npm run build",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "pre-commit": "npm run lint && npm run build && git add lib"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@storybook/addon-actions": "^6.0.27",
    "@storybook/addon-links": "^6.0.27",
    "@storybook/react": "^6.0.27",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "core-js": "^3.6.5",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^4.0.2",
    "husky": "^4.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
